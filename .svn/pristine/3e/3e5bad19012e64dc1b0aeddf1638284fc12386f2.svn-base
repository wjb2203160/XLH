package com.mtr.psn.model.orders;

import java.io.Serializable;

import com.mtr.psn.model.BaseStringTime;

public class AppealForm extends BaseStringTime implements Serializable {

	private static final long serialVersionUID = 1L;
	private String complaintDate;
	private Integer complaintNum;
	private String complaintType;
	private Integer franchiseeID;
	private Integer teamID;
	private String billNum;
	private double complaintAmount;
	private String reason;
	private Integer riderID;

	public String getComplaintDate() {
		return complaintDate;
	}

	public void setComplaintDate(String complaintDate) {
		this.complaintDate = complaintDate;
	}

	public Integer getComplaintNum() {
		return complaintNum;
	}

	public void setComplaintNum(Integer complaintNum) {
		this.complaintNum = complaintNum;
	}

	public String getComplaintType() {
		return complaintType;
	}

	public void setComplaintType(String complaintType) {
		this.complaintType = complaintType;
	}

	public Integer getFranchiseeID() {
		return franchiseeID;
	}

	public void setFranchiseeID(Integer franchiseeID) {
		this.franchiseeID = franchiseeID;
	}

	public Integer getTeamID() {
		return teamID;
	}

	public void setTeamID(Integer teamID) {
		this.teamID = teamID;
	}

	public String getBillNum() {
		return billNum;
	}

	public void setBillNum(String billNum) {
		this.billNum = billNum;
	}

	public double getComplaintAmount() {
		return complaintAmount;
	}

	public void setComplaintAmount(double complaintAmount) {
		this.complaintAmount = complaintAmount;
	}

	public String getReason() {
		return reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public Integer getRiderID() {
		return riderID;
	}

	public void setRiderID(Integer riderID) {
		this.riderID = riderID;
	}

}